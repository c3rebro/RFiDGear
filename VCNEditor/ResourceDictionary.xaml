<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dialogsview="clr-namespace:RFiDGear.UI.MVVMDialogs.View;assembly=RFiDGear"
    xmlns:dialogsvm="clr-namespace:RFiDGear.UI.MVVMDialogs.ViewModels;assembly=RFiDGear"
    xmlns:local="clr-namespace:VCNEditor"
    xmlns:pre="clr-namespace:RFiDGear.UI.MVVMDialogs.Presenters;assembly=RFiDGear"
    xmlns:v="clr-namespace:VCNEditor.View"
    xmlns:vm="clr-namespace:VCNEditor.ViewModel">

    <dialogsview:CustomDialogBox x:Key="{x:Type dialogsvm:CustomDialogViewModel}" />
    <pre:MessageBoxPresenter x:Key="{x:Type dialogsvm:MessageBoxViewModel}" />
    <pre:OpenFileDialogPresenter x:Key="{x:Type dialogsvm:OpenFileDialogViewModel}" />
    <pre:SaveFileDialogPresenter x:Key="{x:Type dialogsvm:SaveFileDialogViewModel}" />

    <v:ScheduleConfigurationDialog x:Key="{x:Type vm:ScheduleConfigurationDialogViewModel}" x:Shared="False" />
    <v:ExpiryConfigurationDialog x:Key="{x:Type vm:ExpiryConfigurationDialogViewModel}" x:Shared="False" />
    <v:ProfileEditor x:Key="{x:Type vm:ProfileEditorViewModel}" x:Shared="False" />

</ResourceDictionary>
